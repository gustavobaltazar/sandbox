// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Anime {
  id     String  @id @default(cuid())
  name   String
  year   String
  season String
  genre  String
  score  Float
  userId String?
}
